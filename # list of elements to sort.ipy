# list of elements to sort
l1= [6, 7, 2, 9, 5]
l2= []
# loop to sort the elements 
for i in range (len (l1)):

    min_val = l1[0]
    for j in range (1, len(l1)):
        if l1[j] < min_val:
         min_val = l1[j]
    l2.append(min_val)

    l1.remove(min_val)

print("In ascending order:",l2)